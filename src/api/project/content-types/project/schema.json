{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "projects",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "project_title": {
      "type": "string"
    },
    "project_desc": {
      "type": "text"
    },
    "project_date": {
      "type": "date"
    },
    "project_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "img_url": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "project_title"
    },
    "tags": {
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    }
  }
}
